  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6e6a86",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": ["string", "string.quoted", "string.template"],
      "settings": {
        "foreground": "#9ccfd8"
      }
    },
    {
      "name": "String interpolation",
      "scope": [
        "punctuation.definition.template-expression",
        "punctuation.section.embedded"
      ],
      "settings": {
        "foreground": "#ebbcba"
      }
    },
    {
      "name": "Escape characters",
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#ebbcba"
      }
    },
    {
      "name": "Numbers",
      "scope": ["constant.numeric"],
      "settings": {
        "foreground": "#f6c177"
      }
    },
    {
      "name": "Boolean & null",
      "scope": [
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined"
      ],
      "settings": {
        "foreground": "#f6c177"
      }
    },
    {
      "name": "Constants",
      "scope": ["variable.other.constant", "variable.other.enummember"],
      "settings": {
        "foreground": "#f6c177"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "keyword.control", "keyword.other"],
      "settings": {
        "foreground": "#c4a7e7"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator", "punctuation.accessor"],
      "settings": {
        "foreground": "#ebbcba"
      }
    },
    {
      "name": "Storage",
      "scope": ["storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#c4a7e7"
      }
    },
    {
      "name": "Import/Export",
      "scope": [
        "keyword.control.import",
        "keyword.control.export",
        "keyword.control.from",
        "keyword.control.as",
        "keyword.control.default"
      ],
      "settings": {
        "foreground": "#ebbcba"
      }
    },
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call entity.name.function",
        "support.function"
      ],
      "settings": {
        "foreground": "#31748f"
      }
    },
    {
      "name": "Function parameters",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#f6c177",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Arrow function",
      "scope": ["storage.type.function.arrow"],
      "settings": {
        "foreground": "#ebbcba"
      }
    },
    {
      "name": "Classes",
      "scope": ["entity.name.class", "entity.name.type.class", "support.class"],
      "settings": {
        "foreground": "#f6c177"
      }
    },
    {
      "name": "Types & Interfaces",
      "scope": [
        "entity.name.type",
        "entity.name.type.interface",
        "entity.name.type.alias",
        "entity.name.type.enum",
        "support.type",
        "support.type.primitive"
      ],
      "settings": {
        "foreground": "#f6c177"
      }
    },
    {
      "name": "Type parameters (generics)",
      "scope": ["entity.name.type.parameter"],
      "settings": {
        "foreground": "#f6c177",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other", "variable.other.readwrite"],
      "settings": {
        "foreground": "#e0def4"
      }
    },
    {
      "name": "Object properties",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "support.variable.property",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#9ccfd8"
      }
    },
    {
      "name": "this/self",
      "scope": [
        "variable.language.this",
        "variable.language.self",
        "variable.language.super"
      ],
      "settings": {
        "foreground": "#eb6f92",
        "fontStyle": "italic"
      }
    },
    {
      "name": "HTML/JSX Tags",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#eb6f92"
      }
    },
    {
      "name": "HTML/JSX Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#c4a7e7",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JSX Component tags",
      "scope": ["support.class.component", "entity.name.tag.component"],
      "settings": {
        "foreground": "#f6c177"
      }
    },
    {
      "name": "Vue directives",
      "scope": [
        "entity.other.attribute-name.directive",
        "keyword.control.directive.vue"
      ],
      "settings": {
        "foreground": "#c4a7e7",
        "fontStyle": "italic"
      }
    },
    {
      "name": "JSON keys",
      "scope": ["support.type.property-name.json"],
      "settings": {
        "foreground": "#31748f"
      }
    },
    {
      "name": "CSS properties",
      "scope": [
        "support.type.property-name.css",
        "support.type.vendored.property-name.css"
      ],
      "settings": {
        "foreground": "#9ccfd8"
      }
    },
    {
      "name": "CSS values",
      "scope": ["support.constant.property-value.css"],
      "settings": {
        "foreground": "#9ccfd8"
      }
    },
    {
      "name": "CSS selectors",
      "scope": [
        "entity.name.tag.css",
        "entity.other.attribute-name.class.css",
        "entity.other.attribute-name.id.css"
      ],
      "settings": {
        "foreground": "#f6c177"
      }
    },
    {
      "name": "Markdown headings",
      "scope": [
        "heading.1.markdown",
        "heading.2.markdown",
        "heading.3.markdown",
        "markup.heading",
        "punctuation.definition.heading.markdown"
      ],
      "settings": {
        "foreground": "#31748f",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown bold",
      "scope": ["markup.bold"],
      "settings": {
        "foreground": "#f6c177",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Markdown italic",
      "scope": ["markup.italic"],
      "settings": {
        "foreground": "#c4a7e7",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Markdown links",
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#9ccfd8"
      }
    },
    {
      "name": "Markdown code",
      "scope": ["markup.inline.raw"],
      "settings": {
        "foreground": "#9ccfd8"
      }
    },
    {
      "name": "Punctuation",
      "scope": [
        "punctuation.separator",
        "punctuation.terminator",
        "punctuation.definition.block",
        "punctuation.definition.parameters",
        "punctuation.definition.array",
        "meta.brace"
      ],
      "settings": {
        "foreground": "#ebbcba"
      }
    },
    {
      "name": "RegExp",
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#9ccfd8"
      }
    },
    {
      "name": "Decorators",
      "scope": ["meta.decorator", "punctuation.decorator"],
      "settings": {
        "foreground": "#f6c177",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Invalid",
      "scope": ["invalid", "invalid.deprecated"],
      "settings": {
        "foreground": "#eb6f92",
        "fontStyle": "strikethrough"
      }
    }
  ],
  "semanticTokenColors": {
    "variable.readonly": "#f6c177",
    "variable.defaultLibrary": "#eb6f92",
    "property.readonly": "#f6c177",
    "enumMember": "#f6c177",
    "namespace": "#f6c177",
    "interface": "#f6c177",
    "type": "#f6c177",
    "class": "#f6c177",
    "function": "#31748f",
    "function.declaration": "#31748f",
    "function.defaultLibrary": "#31748f",
    "method": "#31748f",
    "property": "#9ccfd8",
    "parameter": {
      "foreground": "#f6c177",
      "italic": true
    }
  }
}
